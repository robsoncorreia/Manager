<Page
    x:Class="QRCodeGenerator.View.Generator.GeneratorPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:generator="clr-namespace:QRCodeGenerator.View.Generator"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    x:Name="generatorPage"
    d:DesignHeight="450"
    d:DesignWidth="800"
    Background="{StaticResource BigStoneBillson}"
    DataContext="{Binding Generator, Source={StaticResource Locator}}"
    mc:Ignorable="d">
    <generator:GeneratorUserControl />
</Page>